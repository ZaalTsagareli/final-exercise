{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true
  },
  "projects": {
    "doctor": {
      "type": "library",
      "root": "libs/doctor",
      "entryFile": "index",
      "sourceRoot": "libs/doctor/src",
      "compilerOptions": {
        "tsConfigPath": "libs/doctor/tsconfig.lib.json"
      }
    },
    "doctor-lib": {
      "type": "library",
      "root": "libs/doctor-lib",
      "entryFile": "index",
      "sourceRoot": "libs/doctor-lib/src",
      "compilerOptions": {
        "tsConfigPath": "libs/doctor-lib/tsconfig.lib.json"
      }
    }
  }
}