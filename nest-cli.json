{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true
  },
  "projects": {
    "doctor": {
      "type": "library",
      "root": "libs/doctor",
      "entryFile": "index",
      "sourceRoot": "libs/doctor/src",
      "compilerOptions": {
        "tsConfigPath": "libs/doctor/tsconfig.lib.json"
      }
    },
    "doctor-lib": {
      "type": "library",
      "root": "libs/doctor-lib",
      "entryFile": "index",
      "sourceRoot": "libs/doctor-lib/src",
      "compilerOptions": {
        "tsConfigPath": "libs/doctor-lib/tsconfig.lib.json"
      }
    },
    "common": {
      "type": "library",
      "root": "libs/common",
      "entryFile": "index",
      "sourceRoot": "libs/common/src",
      "compilerOptions": {
        "tsConfigPath": "libs/common/tsconfig.lib.json"
      }
    },
    "patient-lib": {
      "type": "library",
      "root": "libs/patient-lib",
      "entryFile": "index",
      "sourceRoot": "libs/patient-lib/src",
      "compilerOptions": {
        "tsConfigPath": "libs/patient-lib/tsconfig.lib.json"
      }
    },
    "consultation-lib": {
      "type": "library",
      "root": "libs/consultation-lib",
      "entryFile": "index",
      "sourceRoot": "libs/consultation-lib/src",
      "compilerOptions": {
        "tsConfigPath": "libs/consultation-lib/tsconfig.lib.json"
      }
    },
    "offers-lib": {
      "type": "library",
      "root": "libs/offers-lib",
      "entryFile": "index",
      "sourceRoot": "libs/offers-lib/src",
      "compilerOptions": {
        "tsConfigPath": "libs/offers-lib/tsconfig.lib.json"
      }
    },
    "bcrypt-lib": {
      "type": "library",
      "root": "libs/bcrypt-lib",
      "entryFile": "index",
      "sourceRoot": "libs/bcrypt-lib/src",
      "compilerOptions": {
        "tsConfigPath": "libs/bcrypt-lib/tsconfig.lib.json"
      }
    }
  }
}