{
  "compilerOptions": {
    "module": "commonjs",
    "declaration": true,
    "removeComments": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "target": "ES2021",
    "sourceMap": true,
    "outDir": "./dist",
    "baseUrl": "./",
    "incremental": true,
    "skipLibCheck": true,
    "strictNullChecks": false,
    "noImplicitAny": false,
    "strictBindCallApply": false,
    "forceConsistentCasingInFileNames": false,
    "noFallthroughCasesInSwitch": false,
    "paths": {
      "@app/doctor": [
        "libs/doctor/src"
      ],
      "@app/doctor/*": [
        "libs/doctor/src/*"
      ],
      "@app/doctor-lib": [
        "libs/doctor-lib/src"
      ],
      "@app/doctor-lib/*": [
        "libs/doctor-lib/src/*"
      ],
      "@app/common": [
        "libs/common/src"
      ],
      "@app/common/*": [
        "libs/common/src/*"
      ],
      "@app/patient-lib": [
        "libs/patient-lib/src"
      ],
      "@app/patient-lib/*": [
        "libs/patient-lib/src/*"
      ],
      "@app/consultation-lib": [
        "libs/consultation-lib/src"
      ],
      "@app/consultation-lib/*": [
        "libs/consultation-lib/src/*"
      ],
      "@app/offers-lib": [
        "libs/offers-lib/src"
      ],
      "@app/offers-lib/*": [
        "libs/offers-lib/src/*"
      ],
      "@app/bcrypt-lib": [
        "libs/bcrypt-lib/src"
      ],
      "@app/bcrypt-lib/*": [
        "libs/bcrypt-lib/src/*"
      ],
      "@app/redis-lib": [
        "libs/redis-lib/src"
      ],
      "@app/redis-lib/*": [
        "libs/redis-lib/src/*"
      ]
    }
  }
}